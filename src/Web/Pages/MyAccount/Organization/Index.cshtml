@page
@using Web.Interfaces
@model IndexModel
@inject ITokenService TokenService
@{
    ViewData["Title"] = "My organization";
}

@{
    var userName = TokenService.GetUserName();
    
    <h1 class="mb-3">@userName</h1>
}

<div class="mb-3">
    <a asp-page="Info">Organization information</a>
</div>

<div>
    <a asp-page="Events">My events</a>
</div>